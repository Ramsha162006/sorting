#include<iostream>
using namespace std;
void SelectionSort(int arr[],int n)
{
int min = arr[0];
for(int i = 0;i < n-1;i++)
{
for(int j = i+1 ; j<n;j++)
if(arr[j] < min)
{
min = arr[j];
}
}
int temp = min;
min = arr[j];
arr[j] = temp;
}
}
int main()
{
int n;
cin>>n;
int arr[n];
for(int i = 0;i<n;i++)
{   cin>>arr[i];
}
SelectionSort(arr,n);
for(int i = 0;i<n;i++){
cout<<arr[i]<<" ";
}
